    "redux": "^3.5.1",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "stylelint": "^6.5.1",
    "stylelint-config-standard": "^6.0.0",
    "stylelint-loader": "^6.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-postcss-tools": "^1.1.1",
    "whatwg-fetch": "^1.0.0"
