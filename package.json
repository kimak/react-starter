{
  "private": true,
  "name": "react-starter",
  "version": "0.0.1",
  "description": "React Starter",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm test && webpack-dev-server --config webpack.config.js --progress --colors --content-base --host 0.0.0.0 --port 8080",
    "build": "npm run clean && webpack --config webpack.config.js --progress --colors",
    "deploy": "npm run clean && NODE_ENV=production webpack --config webpack.config.js --production",
    "pretest": "npm run clean && webpack --config webpack.config.js --test",
    "test": "mocha dist/test.bundle.js",
    "lint": "stylelint **/*.css",
    "styleguide": "styleguidist server",
    "styleguide-build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimak/react-starter.git"
  },
  "author": "Kimak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kimak/react-starter.git/issues"
  },
  "homepage": "https://github.com/kimak/react-starter.git#readme",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "intl": "^1.2.4",
    "json-loader": "^0.5.4",
    "keymirror": "^0.1.1",
    "mocha": "^2.5.3",
    "postcss-calc": "^5.2.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-loader": "^0.8.2",
    "react": "^15.0.1",
